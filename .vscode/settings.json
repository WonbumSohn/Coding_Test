{
    // Cursor AI 자동완성 비활성화 (0.49.6 버전용)
    "cursor.autocomplete.enabled": false,
    "cursor.inlineCompletions.enabled": false,
    "cursor.copilot.enabled": false,
    "cursor.copilot.chat.enabled": false,
    "cursor.copilot.completions.enabled": false,
    "cursor.copilot.suggestions.enabled": false,
    "cursor.experimental.autocomplete.enabled": false,
    "cursor.experimental.inlineCompletions.enabled": false,
    "cursor.experimental.copilot.enabled": false,
    "cursor.experimental.copilot.chat.enabled": false,
    "cursor.experimental.copilot.completions.enabled": false,
    "cursor.experimental.copilot.suggestions.enabled": false,

    // VSCode 기본 자동완성 비활성화
    "editor.quickSuggestions": {
        "other": false,      // 일반 코드에서 자동완성 비활성화
        "comments": false,   // 주석에서 자동완성 비활성화
        "strings": false     // 문자열 내에서 자동완성 비활성화
    },
    "editor.suggestOnTriggerCharacters": false,  // 특정 문자 입력 시 자동완성 비활성화 (예: . 입력 시)
    "editor.acceptSuggestionOnEnter": "off",     // Enter 키로 자동완성 수락 비활성화
    "editor.inlineSuggest.enabled": false,       // 인라인 제안 비활성화

    // 다양한 종류의 자동완성 제안 비활성화
    "editor.suggest.showMethods": false,         // 메서드 제안 비활성화
    "editor.suggest.showFunctions": false,       // 함수 제안 비활성화
    "editor.suggest.showVariables": false,       // 변수 제안 비활성화
    "editor.suggest.showProperties": false,      // 속성 제안 비활성화
    "editor.suggest.showWords": false,           // 단어 제안 비활성화
    "editor.suggest.showClasses": false,         // 클래스 제안 비활성화
    "editor.suggest.showInterfaces": false,      // 인터페이스 제안 비활성화
    "editor.suggest.showStructs": false,         // 구조체 제안 비활성화
    "editor.suggest.showEnums": false,           // 열거형 제안 비활성화
    "editor.suggest.showEnumMembers": false,     // 열거형 멤버 제안 비활성화
    "editor.suggest.showConstructors": false,    // 생성자 제안 비활성화
    "editor.suggest.showDeprecated": false,      // 더 이상 사용되지 않는 항목 제안 비활성화
    "editor.suggest.showFields": false,          // 필드 제안 비활성화
    "editor.suggest.showFiles": false,           // 파일 제안 비활성화
    "editor.suggest.showFolders": false,         // 폴더 제안 비활성화
    "editor.suggest.showColors": false,          // 색상 제안 비활성화
    "editor.suggest.showTypeParameters": false,  // 타입 매개변수 제안 비활성화
    "editor.suggest.showSnippets": false,        // 코드 스니펫 제안 비활성화
    "editor.suggest.showUsers": false,           // 사용자 제안 비활성화
    "editor.suggest.showIssues": false           // 이슈 제안 비활성화
} 